{
  "rules": {
    "events": {
      "$eventid": {
        "config": {
          ".read": true,
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "profiles": {
          "$uid": {
            ".read": "auth.uid != null",
            ".write": "auth.uid == $uid"
          },
          ".read": "auth.uid != null",
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "submissions": {
          ".read": "auth.uid != null",
          ".write": "auth.uid != null"
        },
        "schedule": {
          ".read": "auth.uid != null",
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "venue": {
          ".read": true,
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "sessions": {
          ".read": true,
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "speakers": {
          ".read": true,
          ".write": "root.child('events').child($eventid).child('/config/admins').hasChild(auth.uid)"
        },
        "feedback": {
          ".read": "auth.uid != null",
          ".write": true
        },
        "favorites": {
          ".read": "auth.uid != null",
          ".write": true
        }
      },
      ".read": true,
      ".write": false
    }
  }
}
